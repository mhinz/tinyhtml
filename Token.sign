SIGNATURE Token

-- IMPORTS {{{1

IMPORT String ONLY string

-- DATA TYPES {{{1

TYPE token == openHtml
              closeHtml

              openTitle
              closeTitle

              openBody
              closeBody

              openParLeft
              openParRight
              openParCenter
              openParJustify
              closePar

              word(data : string)

              error

-- EXPORTS {{{1

FUN `    : token -> denotation
